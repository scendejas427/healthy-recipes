{
	"extends": [],
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts",
			"coverage/lcov-report/*.js"
		]
	},
	"rules": {
		"ordered-imports": false,
		"jsx-self-close": false,
		"jsx-no-lambda": false,
		"no-console": false,
		"no-unused-expression": [false, "allow-fast-null-checks"],
		"jsx-no-bind": false,
		"jsx-boolean-value": false
	}
}
